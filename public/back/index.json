u043e\u0436\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0435\u043a\u0442\u0443 \u0443 \u0436\u0438\u0442\u0442\u044f. \u0410\u0434\u0436\u0435 \u043f\u0440\u0430\u0446\u044e\u0432\u0430\u0442\u0438 \u0442\u0430 \u0436\u0438\u0442\u0438 \u043b\u0435\u0433\u043a\u043e, \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u043e \u0456 \u043a\u0440\u0430\u0441\u0438\u0432\u043e \u043c\u043e\u0436\u043d\u0430, \u043f\u043e\u0442\u0440\u0456\u0431\u043d\u043e \u043b\u0438\u0448\u0435 \u0431\u0430\u0436\u0430\u043d\u043d\u044f. \u041c\u0438 \u0437\u0440\u043e\u0431\u0438\u043c\u043e \u0432\u0441\u0435 \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0449\u043e\u0431 \u043d\u0430\u0448\u043e\u043c\u0443 \u043a\u043b\u0456\u0454\u043d\u0442\u0443 \u0431\u0443\u043b\u043e \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u043e \u043d\u0430 \u0432\u0441\u0456\u0445 \u0435\u0442\u0430\u043f\u0430\u0445 \u0441\u0443\u043c\u0456\u0441\u043d\u043e\u0457 \u0440\u043e\u0431\u043e\u0442\u0438, \u0430 \u0456\u043d\u0442\u0435\u0440'\u0454\u0440 \u0440\u0430\u0434\u0443\u0432\u0430\u0432 \u043d\u0430 \u0434\u043e\u0432\u0433\u0456 \u0440\u043e\u043a\u0438 \u0456 \u0431\u0443\u0432 \u0441\u043f\u0440\u0430\u0432\u0434\u0456 \"\u0412\u0430\u0448\u0438\u043c\".  <\/p><p>\u041c\u0438 \u043f\u0440\u0430\u0446\u044e\u0454\u043c\u043e, \u0449\u043e\u0431 \u0431\u0443\u0442\u0438 \u043d\u0430\u0439\u043a\u0440\u0430\u0449\u043e\u044e \u043c\u0456\u0436\u043d\u0430\u0440\u043e\u0434\u043d\u043e\u044e \u0441\u0442\u0443\u0434\u0456\u0454\u044e \u0434\u0438\u0437\u0430\u0439\u043d\u0443. \u0417\u0456\u0431\u0440\u0430\u0432\u0448\u0438 \u0432 \u0441\u0432\u043e\u044e \u043a\u043e\u043c\u0430\u043d\u0434\u0443 \u043d\u0430\u0439\u043a\u0440\u0430\u0449\u0438\u0445 \u0444\u0430\u0445\u0456\u0432\u0446\u0456\u0432 \u0437 \u0431\u0430\u0433\u0430\u0442\u043e\u0440\u0456\u0447\u043d\u0438\u043c \u0434\u043e\u0441\u0432\u0456\u0434\u043e\u043c, \u043c\u0438 \u0432\u0442\u0456\u043b\u0438\u043c\u043e \u0432\u0430\u0448\u0443 \u043c\u0440\u0456\u044e \u0432\u0456\u0434 \u0456\u0434\u0435\u0457 \u0434\u043e \u0440\u0435\u0430\u043b\u044c\u043d\u043e\u0441\u0442\u0456. \u041a\u043e\u0436\u0435\u043d \u0434\u0435\u043d\u044c \u043c\u0438 \u0440\u043e\u0441\u0442\u0435\u043c\u043e \u0432 \u0441\u0432\u043e\u0457\u0439 \u0434\u0456\u043b\u043e\u0432\u0456\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u0456, \u0449\u043e\u0431 \u0441\u0442\u0432\u043e\u0440\u0438\u0442\u0438 \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u0435 \u0441\u0435\u0440\u0435\u0434\u043e\u0432\u0438\u0449\u0435 \u0434\u043b\u044f \u0432\u0430\u0448\u043e\u0433\u043e \u0436\u0438\u0442\u0442\u044f \u0456 \u0431\u0456\u0437\u043d\u0435\u0441\u0443.<\/p>","block_3_text_2":"<p>\u041d\u0430\u0434\u0438\u0445\u0430\u0454\u043c\u043e\u0441\u044c \u0456 \u0432\u0434\u043e\u0441\u043a\u043e\u043d\u0430\u043b\u044e\u0454\u043c\u043e\u0441\u044c \u043d\u0430 \u043c\u0456\u0436\u043d\u0430\u0440\u043e\u0434\u043d\u0438\u0445 \u043f\u0440\u043e\u0444\u0435\u0441\u0456\u0439\u043d\u0438\u0445 \u0432\u0438\u0441\u0442\u0430\u0432\u043a\u0430\u0445 \u0456 \u043f\u0440\u043e\u0444\u0456\u043b\u044c\u043d\u0438\u0445 \u0437\u0430\u0445\u043e\u0434\u0430\u0445. \u041e\u0441\u043e\u0431\u0438\u0441\u0442\u043e \u0441\u043f\u0456\u043b\u043a\u0443\u0454\u043c\u043e\u0441\u044f \u0437 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043d\u0438\u043a\u0430\u043c\u0438 \u043c\u043e\u0434\u043d\u0438\u0445 \u0431\u0440\u0435\u043d\u0434\u0456\u0432 \u0435\u043a\u0441\u043a\u043b\u044e\u0437\u0438\u0432\u043d\u0438\u0445 \u043c\u0435\u0431\u043b\u0456\u0432 \u0456 \u0447\u0430\u0441\u0442\u043e \u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u043e\u0432\u0443\u0454\u043c\u043e \u0432 \u043d\u0430\u0448\u0438\u0445 \u043f\u0440\u043e\u0435\u043a\u0442\u0430\u0445 \u043c\u0435\u0431\u043b\u0456, \u0432\u0438\u043a\u043e\u043d\u0430\u043d\u0456 \u0437\u0430 \u0456\u043d\u0434\u0438\u0432\u0456\u0434\u0443\u0430\u043b\u044c\u043d\u0438\u043c\u0438 \u0435\u0441\u043a\u0456\u0437\u0430\u043c\u0438.<\/p>","block_3_text_3":"<p>\u041f\u0440\u043e\u043f\u043e\u043d\u0443\u044e\u0447\u0438 \u0443\u043d\u0456\u043a\u0430\u043b\u044c\u043d\u0443 \u043c\u0435\u0442\u043e\u0434\u0438\u043a\u0443 \u0440\u043e\u0431\u043e\u0442\u0438 \u0437 \u0437\u0430\u043c\u043e\u0432\u043d\u0438\u043a\u043e\u043c \u0456 \u0432\u0438\u0441\u043e\u043a\u043e\u043a\u043b\u0430\u0441\u043d\u0438\u0439 \u0441\u0435\u0440\u0432\u0456\u0441, \u043c\u0438 \u0437\u0430\u0434\u043e\u0432\u0456\u043b\u044c\u043d\u044f\u0454\u043c\u043e \u0435\u0441\u0442\u0435\u0442\u0438\u0447\u043d\u0438\u043c\u0438 \u043f\u043e\u0442\u0440\u0435\u0431\u0430\u043c\u0438 \u043a\u043b\u0456\u0454\u043d\u0442\u0456\u0432. \u0417\u043d\u0456\u043c\u0430\u0454\u043c\u043e \u0437 \u043d\u0438\u0445 \u0432\u0441\u0456 \u0442\u0443\u0440\u0431\u043e\u0442\u0438, \u0434\u0430\u0454\u043c\u043e \u043c\u043e\u0436\u043b\u0438\u0432\u0456\u0441\u0442\u044c \u0431\u0443\u0442\u0438 \u0432 \u0442\u0440\u0435\u043d\u0434\u0456, \u0440\u043e\u0431\u043b\u044f\u0447\u0438 \u0457\u0445 \u0436\u0438\u0442\u0442\u044f \u043a\u0440\u0430\u0449\u0438\u043c, \u043a\u0440\u0430\u0441\u0438\u0432\u0456\u0448\u0438\u043c \u0442\u0430 \u043a\u043e\u043c\u0444\u043e\u0440\u0442\u043d\u0456\u0448\u0438\u043c. \u0406 \u043d\u0430\u0439\u043a\u0440\u0430\u0449\u0435, \u0449\u043e \u043c\u043e\u0436\u0435 \u0437\u0440\u043e\u0431\u0438\u0442\u0438 \u043a\u043b\u0456\u0454\u043d\u0442-\u0446\u0435 \u0437\u0432\u0435\u0440\u043d\u0443\u0442\u0438\u0441\u044c \u0434\u043e \u043d\u0430\u0441 \u043f\u043e\u0432\u0442\u043e\u0440\u043d\u043e \u0434\u043b\u044f \u0441\u0442\u0432\u043e\u0440\u0435\u043d\u043d\u044f \u043d\u0430\u0441\u0442\u0443\u043f\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0435\u043a\u0442\u0443.<\/p>","block_4_title_1":"\u0414\u0438\u0437\u0430\u0439\u043d \u0456\u043d\u0442\u0435\u0440'\u0454\u0440\u0443","block_4_text_1":"<p>\u0412 DOREALE DESIGN \u0